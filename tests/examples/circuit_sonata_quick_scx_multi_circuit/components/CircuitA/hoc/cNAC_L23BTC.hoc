  }
  forsec this.apical {
    insert CaDynamics_DC0
    insert Ca_HVA2
    insert Ca_LVAst
    insert SK_E2
    insert Ih
  }
  forsec this.axonal {
    insert CaDynamics_DC0
    insert Ca_HVA2
    insert Ca_LVAst
    insert SK_E2
    insert NaTg
    insert SKv3_1
    insert K_Pst
    insert K_Tst
  }
  forsec this.basal {
    insert CaDynamics_DC0
    insert Ca_HVA2
    insert Ca_LVAst
    insert SK_E2
    insert Ih
  }
  forsec this.somatic {
    insert CaDynamics_DC0
    insert Ca_HVA2
    insert Ca_LVAst
    insert SK_E2
    insert NaTg
    insert SKv3_1
    insert K_Pst
    insert K_Tst
    insert Ih
  }
  forsec this.myelinated {
  }
}

