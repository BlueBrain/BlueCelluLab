{
    "version": 1,
    "manifest": {
        "$OUTPUT_DIR": "./reporting",
        "$INPUT_DIR": "./"
    },
    "run": {
        "tstart": 0,
        "tstop": 1.0,
        "dt": 0.1,
        "random_seed": 123
    },
    "output": {
        "output_dir": "$OUTPUT_DIR",
        "spikes_file": "spikes.h5",
        "spikes_sort_order": "by_time"
    },
    "reports": {
        "soma_report": {
            "cells": "node_set1",
            "variable_name": "v",
            "sections": "soma"
        },
        "compartment_report": {
            "cells": "node_set2",
            "variable_name": "v",
            "sections": "all"
        }
    }
}